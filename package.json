{
  "name": "optchain-v2",
  "version": "0.1.0",
  "private": true,
  "description": "CopilotKit-enabled version of OptionChain web application",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:backend": "cd backend && python -m pytest ../tests/backend -v --cov=. --cov-report=term-missing",
    "test:all": "npm run test:run && npm run test:backend"
  },
  "dependencies": {
    "@copilotkit/react-core": "^1.3.0",
    "@copilotkit/react-ui": "^1.3.0",
    "@copilotkit/runtime": "^1.3.0",
    "@google-cloud/storage": "^7.7.0",
    "@google/generative-ai": "^0.21.0",
    "autoprefixer": "^10.4.0",
    "next": "^14.2.33",
    "postcss": "^8.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "tailwindcss": "^3.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.5.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
